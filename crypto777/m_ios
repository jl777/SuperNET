
CC="$(xcrun --sdk iphoneos --find clang) -isysroot $(xcrun --sdk iphoneos --show-sdk-path) -arch arm64 -arch armv7 -arch armv7s -ONLY_ACTIVE_ARCH=YES"
AR="$(xcrun --sdk iphoneos --find ar)"

echo Compiling ..

$CC -Wno-deprecated -I ../ios/include -c  -O2 *.c jpeg/*.c jpeg/unix/*.c -I/usr/local/opt/
rm -f ../agents/libcrypto777.a; $AR -rcs ../agents/libcrypto777.a *.o 
