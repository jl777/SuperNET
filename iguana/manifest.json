{
    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "manifest_version": 2,
    "name": "Iguana",
    "version": "0.0.1",
    "icons": {
        "128": "iguana/icon128.png"
    },
    "minimum_chrome_version": "36",
    "sockets": {
        "tcp": {
            "connect": "*"
        },
        "tcpServer": {
            "listen": "*"
        }
    },
    "permissions": [
                    "storage"
                    ]
}
